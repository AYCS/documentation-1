{
  "IndexDocument": {
    "Suffix": "index.html"
  },
  "ErrorDocument": {
    "Key": "404/index.html"
  },
  "RoutingRules": [{
    "Condition": {
      "KeyPrefixEquals": "docker-integration/google-cloud/"
    },
    "Redirect": {
      "HostName": "documentation.codeship.com",
      "ReplaceKeyPrefixWith": "docker/continuous-deployment/google-cloud/",
      "HttpRedirectCode": "302"
    }
  },
  {
    "Condition": {
      "KeyPrefixEquals": "continuous-integration/ssh-access/"
    },
    "Redirect": {
      "HostName": "documentation.codeship.com",
      "ReplaceKeyPrefixWith": "classic/getting-started/ssh-access/",
      "HttpRedirectCode": "302"
    }
  },
  {
    "Condition": {
      "KeyPrefixEquals": "continuous-integration/browser-testing/"
    },
    "Redirect": {
      "HostName": "documentation.codeship.com",
      "ReplaceKeyPrefixWith": "classic/continuous-integration/browser-testing/",
      "HttpRedirectCode": "302"
    }
  },
  {
    "Condition": {
      "KeyPrefixEquals": "docker-guide/getting-started/"
    },
    "Redirect": {
      "HostName": "documentation.codeship.com",
      "ReplaceKeyPrefixWith": "docker/getting-started/getting-started/",
      "HttpRedirectCode": "302"
    }
  },
  {
    "Condition": {
      "KeyPrefixEquals": "continuous-integration/set-environment-variables/"
    },
    "Redirect": {
      "HostName": "documentation.codeship.com",
      "ReplaceKeyPrefixWith": "classic/getting-started/set-environment-variables/",
      "HttpRedirectCode": "302"
    }
  }]
}
